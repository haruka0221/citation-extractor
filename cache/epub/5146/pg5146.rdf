    <dcterms:title>The Fighting Governor : A Chronicle of Frontenac</dcterms:title>
    <pgterms:marc260>Toronto : [s.n.], $c 1915</pgterms:marc260>
    <pgterms:marc440>Chronicles of Canada series: Volume 07</pgterms:marc440>
